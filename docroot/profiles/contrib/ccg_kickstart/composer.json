{
  "name": "miggle/ccg_kickstart",
  "description": "The CCG Kickstart installation profile",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "drush/drush": "^9.0",
    "drupal/drupal-extension": "^3.2",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "behat/behat": "^3.0",
    "drupal/coder": "8.*",
    "phing/phing": "^2.14",
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "~2.8",
    "drupal/devel": "^8.0.0",
    "squizlabs/php_codesniffer": "2.*"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.drupal-composer.org"
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "@composer drupal-scaffold",
      "./bin/phing push"
    ],
    "post-update-cmd": [
      "./bin/phing push"
    ],
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  },
  "extra": {
    "installer-paths": {
      "docroot/core": [
        "drupal/core"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ]
    },
    "patches": {
      "drupal/core": {
        "2635712 - Quickedit delete files": "https://www.drupal.org/files/issues/2635712-20.patch"
      }
    }
  },
  "require": {
    "cweagans/composer-patches": "^1.5.0",
    "composer/installers": "^1.0",
    "drupal/lightning": "^8.1.0",
    "drupal/admin_toolbar": "^8.1.0",
    "drupal/field_group": "^8.1.0",
    "drupal/pathauto": "^8.1.0",
    "drupal/search_api": "^8.1.0",
    "drupal/file_browser": "^8.1.0",
    "drupal/file_entity": "^8.2.0",
    "drupal/twitter_block": "^8.2.0",
    "drupal/search_api_page": "^8.1.0",
    "drupal/sitemap": "^8.1.0",
    "drupal/default_content": "^8.1.0",
    "drupal/cookieconsent": "^8.1.0",
    "drupal/social_media_links": "^8.1.0",
    "drupal/sharethis": "^8.2.0",
    "drupal/menu_block": "^8.1.0",
    "drupal/config_rewrite": "^8.1.0",
    "drupal/contact_block": "^8.1.0"
  }
}
